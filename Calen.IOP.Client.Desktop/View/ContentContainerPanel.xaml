<UserControl x:Class="Calen.IOP.Client.Desktop.View.ContentContainerPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Calen.IOP.Client.Desktop.View"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="42"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Border Background="{DynamicResource EdgeBarColorLevel1}">
            <TextBlock Name="tb_Title" FontSize="{DynamicResource FontSize_Large}" Foreground="{DynamicResource Foreground_TitleKey}" Text="主要功能" VerticalAlignment="Center" Margin="6,0,0,0"/>
        </Border>
        <Frame Name="frame" Grid.Row="1" NavigationUIVisibility="Hidden">
            
        </Frame>
    </Grid>
</UserControl>
