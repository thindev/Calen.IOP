<Page x:Class="Calen.IOP.Client.Desktop.Pages.Common.VIPCardConfigPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:mahapps="http://metro.mahapps.com/winfx/xaml/controls"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Calen.IOP.Client.Desktop.Pages.Common"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="会员卡类型配置">
    <TabControl Style="{DynamicResource PageTabControlStyle}">
        <TabItem Header="会员卡类型配置" Style="{DynamicResource PageTabItemStyle}">
            <mahapps:MetroContentControl>
                <local:VIPCardConfigPanel/>
            </mahapps:MetroContentControl>
        </TabItem>
    </TabControl>
</Page>
